{
  "defaultSeverity": "warn",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name": [false],
    "object-literal-sort-keys": [false],
    "jsx-no-lambda": [false],
    "no-console": [false],
    "no-unused-variable": [true],
    "no-unused-expression": [true]
  }
}
